class A {
    public static void main(String args[]) {
        int n = 10;
        int sum = 0;

        for (int i = 1; i < n; i++) {
            if (n % i == 0) {
                System.out.println(i);
                sum += i;
            }
        }

        if (sum == n) {
            System.out.println(sum + " is a perfect number");
        } else {
            System.out.println(sum + " is NOT a perfect number");
        }
    }
}
