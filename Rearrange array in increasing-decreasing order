import java.util.*;
class B{
void get(int arr[]){
Arrays.sort(arr);
for(int i=0;i<arr.length/2;i++){
System.out.println(arr[i]);
}

for(int i=arr.length-1;i>=arr.length/2;i--){
System.out.println(arr[i]);
}
}
}
class A{
public static void main(String args[]){
int arr[]={8,7,1,6,5,9};
B ans=new B();
ans.get(arr);
}
}
