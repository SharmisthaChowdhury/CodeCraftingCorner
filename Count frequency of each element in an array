import java.util.*;
class A{
public static void main(String args[]){
int arr[]={10, 5, 10, 15, 10, 5};
int count=1;
Arrays.sort(arr);
for(int i=1;i<arr.length;i++){
if(arr[i]==arr[i-1]){
count++;
}
else{
System.out.println(arr[i-1]+"  "+count);
count=1;
}
}
    System.out.println(arr[arr.length - 1] + "  " + count);
}
}
