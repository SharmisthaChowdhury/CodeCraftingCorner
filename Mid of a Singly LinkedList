import java.util.*;
class A{
public static void main(String args[]){
LinkedList<Integer> l=new LinkedList<>();
for(int i=1;i<=20;i++){
l.add(i);
}
for(int i=0;i<l.size();i++){
System.out.print(l.get(i)+" ");
}

int left=0;
int right=l.size()-1;
int mid=(left+right)/2;
System.out.println("mid: "+mid);
}
}
