import java.util.*;

class A{
void get(int arr[]){
Arrays.sort(arr);
int n=arr.length;
int sum=0;
int avg=0;
if(n%2==0){
System.out.println(arr[n/2]);
}
else{
for(int i=0;i<n;i++){
sum +=arr[i];
 avg=sum/n;
}
System.out.println(avg);
}
}

public static void main(String args[]){
int arr[]={2,4,1,3,5};
int n=5;
A a=new A();
a.get(arr);
}
}
