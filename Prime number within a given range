class A {
    public static void main(String[] args) {
        int n = 24;
        for (int num = 2; num <= n; num++) {
            boolean prime = true;
            for (int i = 2; i < num; i++) {
                if (num % i == 0) {
                    prime = false;
                    break;
                }
            }
            if (prime) {
                System.out.println("prime: " + num);
            }         }
    }
}
